<Window x:Class="WPFEditor.MapSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Map Selection" Height="300" Width="300"
        WindowStartupLocation="CenterOwner">
    <Grid Background="Black">
        <Button Background="Blue" BorderBrush="White" Content="Load Map" Focusable="False" FontFamily="Times New Roman" FontSize="14" FontWeight="Bold" Foreground="White" Height="23" HorizontalAlignment="Right" Margin="0,10,135,0" Name="loadButton" VerticalAlignment="Top" Width="133" Click="loadButton_Click" />
        <ScrollViewer Height="222" HorizontalAlignment="Left" Margin="0,39,0,0" Name="scrollViewer1" VerticalAlignment="Top" Width="278" VerticalScrollBarVisibility="Auto" CanContentScroll="True">
            <Grid Name="grid1" />
        </ScrollViewer>
        <Button Background="Blue" BorderBrush="White" Content="Cancel" Focusable="False" FontFamily="Times New Roman" FontSize="14" FontWeight="Bold" Foreground="White" Height="23" HorizontalAlignment="Right" Margin="0,10,12,0" Name="cancelButton" VerticalAlignment="Top" Width="117" Click="cancelButton_Click" />
    </Grid>
</Window>
