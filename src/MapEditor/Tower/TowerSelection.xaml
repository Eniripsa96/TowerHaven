<Window x:Class="WPFEditor.TowerSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tower Selection" Height="300" Width="300"
        WindowStartupLocation="CenterOwner" ResizeMode="CanResize">
    <Grid Background="Black">
        <Button Background="Blue" BorderBrush="White" Content="Edit Tower" Focusable="False" FontFamily="Times New Roman" FontSize="14" FontWeight="Bold" Foreground="White" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="doneButton" VerticalAlignment="Top" Width="143" Click="editButton_Click" />
        <ScrollViewer Height="Auto" HorizontalAlignment="Stretch" Margin="0,41,0,0" Name="scrollViewer1" VerticalAlignment="Stretch" Width="Auto" VerticalScrollBarVisibility="Auto" CanContentScroll="True">
            <Grid Name="grid1" Margin="0,0,0,0" />
        </ScrollViewer>
        <Ellipse Name="ellipse1" Stroke="Black" Margin="229,-49,-42,218">
            <Ellipse.Fill>
                <RadialGradientBrush>
                    <GradientStop Color="Black" Offset="0" />
                    <GradientStop Color="White" Offset="1" />
                    <GradientStop Color="#FFB0B0B0" Offset="0.856" />
                </RadialGradientBrush>
            </Ellipse.Fill>
        </Ellipse>
    </Grid>
</Window>
